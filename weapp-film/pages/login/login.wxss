page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.weui-navbar{
    background-color: #fff;
}
.weui-navbar__item.weui-bar__item_on{
    color: #FF4D64;
}
.weui-navbar__slider{
    background-color: #FF4D64;
}
.weui-tab__panel{
    background-color: #FBF9FE;
}
.weui-label{
    width: 150rpx;
    font-size: 40rpx;
}
.weui-input{
    font-size: 35rpx;
}
.weui-vcode-btn{
    font-size: 35rpx;
    padding: 0 5rpx;
    height: 1.8em;
    line-height: 1.8em;
    margin-right: 10rpx;
    border: 2rpx solid #999;
    border-radius: 5rpx;
    color: #999;
}
.weui-btn{
    background-color: #FF4D64;
    color: #fff;
}
.other-login{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 100rpx;
    color: #999;
    font-size: 35rpx;
}
.other-login-tit{
    position: relative;
}
.other-login-tit::before{
    position: absolute;
    content: '';
    width: 100rpx;
    border: 1rpx dotted #eee;
    height: 0;
    top: 50%;
    left: -120rpx;
}
.other-login-tit::after{
    position: absolute;
    content: '';
    width: 100rpx;
    border: 1rpx dotted #eee;
    height: 0;
    top: 50%;
    right: -120rpx;
}
.other-login-icon{
    width: 150rpx;
    height: 150rpx;
    margin-top: 40rpx;
    border-radius: 50%;
    background-image: url(data:image/png;base64,);
    background-position: center;
    background-size: 100rpx 100rpx;
    background-repeat: no-repeat;
    background-color: #fff;
}